@model double?

@using MVC5Course.Models

@{
	var db = new FabricsEntities();
	var rating_list = (from p in db.Client select p.CreditRating).Distinct().OrderBy(p => p).ToList();
	var CreditRating = new SelectList(rating_list, Model);
}

@Html.DropDownList("",CreditRating,"--請選擇--", new { onchange = "this.form.submit();" , @class = "form-control" })

